{
    "name": "fuel-pjax",
    "version": "1.2.3",
    "description": "Easily enable fast AJAX navigation using pushState + XMLHttpRequest Prefetching",
    "keywords": [
        "pjax",
        "pushstate",
        "ajax",
        "navigation",
        "transition",
        "animation",
        "prefetcher"
    ],
    "repository": "https://github.com/Pageworks/fuel-pjax.git",
    "author": "Kyle Andrews",
    "contributors": [
        "Kyle Andrews (http://devkyle.com)"
    ],
    "license": "MIT",
    "main": "pjax.js",
    "types": "./src/global.d.ts",
    "files": [
        "src",
        "lib",
        "pjax.js",
        "pjax.js.map",
        "pjax.d.ts"
    ],
    "husky": {
        "hooks": {
            "pre-commit": "npm run precommit"
        }
    },
    "dependencies": {
        "fuel-device-manager": "latest",
        "fuel-state-manager": "latest"
    },
    "devDependencies": {
        "http-server": "^0.11.1",
        "husky": "^0.14.3",
        "npm-run-all": "^4.1.5",
        "tslint": "^5.14.0",
        "tslint-config-airbnb": "^5.11.1",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^3.3.3333",
        "webpack": "^4.29.6",
        "webpack-cli": "^3.3.0"
    },
    "scripts": {
        "precommit": "tslint -c .tslintrc.js -p tsconfig.json -t verbose",
        "compile": "tsc",
        "bundle": "webpack",
        "demo": "http-server ./testing",
        "test": "npm run compile && npm run bundle && npm run demo"
    }
}
